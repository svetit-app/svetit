{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "X-User": {
      "format": "uuid",
      "in": "header",
      "type": "string"
    },
    "limit": {
      "format": "int32",
      "in": "query",
      "maximum": 1000,
      "type": "integer"
    },
    "start": {
      "format": "int32",
      "in": "query",
      "maximum": 999,
      "type": "integer"
    }
  },
  "required": [
    "limit",
    "start",
    "X-User"
  ],
  "type": "object"
}
